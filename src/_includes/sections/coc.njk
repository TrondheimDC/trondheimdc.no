<tdc-section>
<section class="section section--coc" id="coc" data-animate aria-labelledby="coc-title">
  <div class="container container--narrow">
    <div class="section__header">
      <h2 class="section__title" id="coc-title">{{ t.coc.title }}</h2>
    </div>
    <div class="stack">
      <p>{{ t.coc.body }}</p>
      <p>{{ t.coc.body2 }}</p>
      <p>{{ t.coc.body3 }}</p>
      <p>{{ t.coc.body4 }}</p>
      <p><strong>{{ t.coc.report }}</strong></p>
    </div>
  </div>
</section>
</tdc-section>
